# E80 Computer - Pin Assignments for the Olimex GateMate A1-EVB
# Copyright (C) 2026 Panos Stokas <panos.stokas@hotmail.com>
# Includes a quick reference for onboard components and GPIO banks.

# Onboard 10MHz crystal oscillator CLK0 (frequency is specified at Board.vhd)
NET "BoardCLK" Loc = "IO_SB_A8";

# MAX7219 4x8x8 LED Matrix inputs 
NET "MAX7219DIN" Loc = "IO_NB_B0";
NET "MAX7219CS"  Loc = "IO_NB_B1";
NET "MAX7219CLK" Loc = "IO_NB_B2";

# Joystick controls; SCHMITT_TRIGGER is enabled to prevent bouncing
NET "Left"        Loc = "IO_EB_B4" | SCHMITT_TRIGGER=true; # 10
NET "Right"       Loc = "IO_EB_B3" | SCHMITT_TRIGGER=true; # 12
NET "SetButton"   Loc = "IO_EB_B1" | SCHMITT_TRIGGER=true; # 16
NET "ResetButton" Loc = "IO_EB_B0" | SCHMITT_TRIGGER=true; # 18

# DIP switches
NET "DIPinput[0]" Loc = "IO_EB_A7" | SCHMITT_TRIGGER=true; # 5
NET "DIPinput[1]" Loc = "IO_EB_A6" | SCHMITT_TRIGGER=true; # 7
NET "DIPinput[2]" Loc = "IO_EB_A5" | SCHMITT_TRIGGER=true; # 9
NET "DIPinput[3]" Loc = "IO_EB_A4" | SCHMITT_TRIGGER=true; # 11
NET "DIPinput[4]" Loc = "IO_EB_A3" | SCHMITT_TRIGGER=true; # 13
NET "DIPinput[5]" Loc = "IO_EB_A2" | SCHMITT_TRIGGER=true; # 15
NET "DIPinput[6]" Loc = "IO_EB_A1" | SCHMITT_TRIGGER=true; # 17
NET "DIPinput[7]" Loc = "IO_EB_A0" | SCHMITT_TRIGGER=true; # 19

